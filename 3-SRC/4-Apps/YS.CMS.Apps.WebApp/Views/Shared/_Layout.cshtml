<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - YS.CMS.Apps.WebApp</title>
    <environment include="Development">
        <link rel="stylesheet" href="~/dist/css/vendor.css" />
        <link rel="stylesheet" href="~/dist/css/dashboard.css" />
        <link rel="stylesheet" href="~/dist/css/app.css" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="~/dist/css/vendor.min.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/dist/css/dashboard.min.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/dist/css/app.min.css" asp-append-version="true" />
    </environment>
</head>
<body style="background-color:#51A8B1;">
    @RenderBody()
    <environment include="Development">
        <script src="~/dist/js/vendor.js"></script>
        <script src="~/dist/js/dashboard.js"></script>
        <script src="~/dist/js/app.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/dist/js/vendor.min.js" asp-append-version="true"></script>
        <script src="~/dist/js/dashboard.min.js" asp-append-version="true"></script>
        <script src="~/dist/js/app.min.js" asp-append-version="true"></script>
    </environment>
    @RenderSection("Scripts", required: false)
</body>
</html>
